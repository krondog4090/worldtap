{
  "expo": {
    "sdkVersion": "31.0.0",
    "name": "World Tap",
    "slug": "World-Tap",
    "description": "Players playing for their countries",
    "version": "0.2.4",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "privacy": "public",
    "icon": "./assets/images/wtappicon.png",
    "assetBundlePatterns": [
      "./assets/images/*/**",
      "./assets/fonts/*",
      "./assets/audio/*",
      "./assets/sprites/*"
    ],
    "splash": {
      "backgroundColor": "#000000",
      "image": "./assets/images/krondogsplashfinal.png",
      "resizeMode": "cover"
    },
    "ios": {
      "bundleIdentifier": "com.blakemcnary.worldtap",
      "supportsTablet": true
    },
    "android": {
      "package": "com.blakemcnaryandroid.worldtap",
      "permissions": []
    }
  }
}
